{
  "name": "terllo",
  "version": "1.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run start:server\" \"npm run start:frontend\"",
    "start:server": "NODE_ENV=development BABEL_ENV=server nodemon server/index.js",
    "start:frontend": "NODE_ENV=development BABEL_ENV=frontend webpack -w",
    "test:server": "jest server/tests/**/*.test.js --watch",
    "deploy": "NODE_ENV=production npm run build:frontend && npm run build:server && ./bin/deploy.sh",
    "build:frontend": "BABEL_ENV=frontend webpack -p",
    "build:server": "BABEL_ENV=server babel server -d dist/server"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.0",
    "graphql-tools": "^2.13.0",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^4.13.6",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "1.4.16",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.9.1",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
